<%= form_with(model: user_workout, local: true) do |form| %>
    <p>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
    </p>
    <p>
      <%= form.label :description %><br>
      <%= form.text_area :description %>
    </p>
    <p>
      <%= form.label :sets %><br>
      <%= form.number_field :sets %>
    </p>
    <p>
      <%= form.label :reps %><br>
      <%= form.number_field :reps %>
    </p>
    <p>
      <%= form.label :date %><br>
      <%= form.date_select :date %>
    </p>
    <p>
      <%= form.submit %>
    </p>
  <% end %>
  